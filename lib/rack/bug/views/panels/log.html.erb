<h3>Log Messages</h3>
<table>
  <thead>
    <tr>
      <th>Level</th>
      <th>Time</th>
      <th>Message</th>
    </tr>
  </thead>
  <tbody>
    <% i = 1 %>
    <% logs.each do |entry| %>
      <tr class="<%= i % 2 == 0 ? "even" : "odd" %>">
        <td><%= entry.level %></td>
        <td><%= entry.time %>ms</td>
        <td><%= entry.cleaned_message %></td>
      </tr>
      <% i += 1 %>
    <% end %>
  </tbody>
</table>